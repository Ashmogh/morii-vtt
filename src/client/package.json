{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "build-with-css": "yarn prepare-theme && yarn build",
    "build-prod": "yarn install --frozen-lockfile && yarn build-with-css",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "prepare-theme": "smui-theme compile public/static/smui-dark.css -i src/theme"
  },
  "devDependencies": {
    "@material/elevation": "^13.0.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@smui/card": "^6.0.0-beta.14",
    "@smui/fab": "^6.0.0-beta.14",
    "@smui/icon-button": "^6.0.0-beta.14",
    "@smui/menu": "^6.0.0-beta.14",
    "@smui/paper": "^6.0.0-beta.14",
    "@smui/ripple": "^6.0.0-beta.14",
    "@smui/segmented-button": "^6.0.0-beta.13",
    "@smui/snackbar": "^6.0.0-beta.14",
    "@smui/textfield": "^6.0.0-beta.14",
    "@smui/tooltip": "^6.0.0-beta.14",
    "@tsconfig/svelte": "^2.0.0",
    "postcss": "^8.4.5",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-dev": "^2.0.2",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sass": "^1.47.0",
    "smui-theme": "^6.0.0-beta.13",
    "svelte": "^3.0.0",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.10.1",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0"
  },
  "dependencies": {
    "@mdi/font": "^6.5.95",
    "@smui/circular-progress": "^6.0.0-beta.14",
    "@smui/select": "^6.0.0-beta.14",
    "@smui/switch": "^6.0.0-beta.15",
    "mem": "^9.0.2",
    "sirv-cli": "^2.0.0",
    "socket.io-client": "^4.4.1",
    "svelte-copy-to-clipboard": "^0.2.5",
    "svelte-markdown": "^0.2.2",
    "svelte-material-ui": "^6.0.0-beta.13",
    "svelte-spa-router": "^3.2.0"
  }
}
